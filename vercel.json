{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" },
    {
      "source": "/api/:path*",
      "destination": "https://server-nest-khaki.vercel.app/api/v1/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
